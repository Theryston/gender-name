{
  "name": "gender-name",
  "version": "1.1.0",
  "description": "An API that uses artificial intelligence to predict someone's gender based on their name",
  "author": "Theryston Santos",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build && yarn seed",
    "start:offline": "yarn serverless offline --httpPort 3331",
    "start:dev": "nodemon --watch src -e ts --exec \"yarn build && yarn start:offline\"",
    "deploy:aws": "yarn build && yarn serverless deploy --stage prod --verbose",
    "undeploy:aws": "yarn serverless remove --stage prod --verbose",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "seed": "ts-node ./src/database/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "dotenv": "^16.0.3",
    "mongodb": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "replicate": "^0.22.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.3.2",
    "serverless": "^3.30.1",
    "serverless-domain-manager": "^7.0.3",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
